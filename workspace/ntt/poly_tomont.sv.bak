module poly_tomont (
  input  signed [15:0] in  [0:255],
  output signed [15:0] out [0:255]
);

  localparam signed [15:0] MONT_R2 = 16'sd1353;

  genvar i;
  generate
    for (i = 0; i < 256; i = i + 1) begin : g_mont
      fqmul mul (
        .a(in[i]),
        .b(MONT_R2),
        .r(out[i])
      );
    end
  endgenerate
endmodule
